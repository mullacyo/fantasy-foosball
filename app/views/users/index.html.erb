<div class="container card" style="width: 30rem;">
    <div class="card-body">
        <h1 class="display-4 text-center">All Users</h1>
        <% @users.each do |user| %>
            <%= user.id %>.
            <%= user.first_name %>
            <%= user.last_name %>
            <br>
        <% end %>
    </div>
</div>
